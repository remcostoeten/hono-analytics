{
  "name": "honolytics",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "bun run --filter docs dev",
    "docs": "cd apps/docs; bun run dev",
    "docs:build": "cd apps/docs; bun run build",
    "deploy": "./scripts/deploy-blog.sh",
    "build": "bun run --filter honolytics build",
    "build:all": "bun run build && bun run --filter docs build"
  },
  "dependencies": {
    "ci-info": "^4.3.0",
    "create-fumadocs-app": "^15.8.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.37.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "prettier": "^3.6.2"
  }
}
