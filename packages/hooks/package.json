{
  "name": "@onolythics/hooks",
  "version": "2.0.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc --emitDeclarationOnly --declaration --outDir dist",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "description": "React hooks for building analytics dashboards with Onolythics",
  "author": "Remco Stoeten",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/remcostoeten/hono-analytics.git",
    "directory": "packages/hooks"
  },
  "bugs": {
    "url": "https://github.com/remcostoeten/hono-analytics/issues"
  },
  "homepage": "https://github.com/remcostoeten/hono-analytics#readme",
  "keywords": [
    "onolythics",
    "analytics",
    "dashboard",
    "react",
    "hooks",
    "typescript",
    "hono",
    "self-hosted",
    "privacy-focused"
  ],
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.11",
    "react": "^19.1.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  }
}
