# Deployment Environment Variables for Hono Analytics Backend
# Copy this file to .env.deploy and fill in your values

# Required: Neon Database Connection String
DATABASE_URL="postgresql://user:password@host.neon.tech/dbname?sslmode=require"

# Optional: CORS Origins (comma-separated list)
# Default: "http://localhost:3000,https://your-app-name.fly.dev"
# CORS_ORIGIN="https://your-frontend.com,https://your-docs.com"

# Optional: API Key for initial project
# Default: Auto-generated secure key (will be displayed during deployment)
# DEFAULT_API_KEY="your-secure-api-key-here"

# Optional: Log Level
# Default: "info"
# LOG_LEVEL="debug"
